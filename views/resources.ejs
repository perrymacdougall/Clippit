        <% include ./partials/_header %>


  <!-- Add EJS code here -->
    <div class="container" style="margin-top:200px;">  
      <div class="row">
        <h2>Resources</h2>
      </div>
      <div class="row">
      <% for(let row in rows) { %>
      <div class="col-4">
              
        <a href="/resources/<%= rows[row].resource_id %>">
          <div class="resource">
            <div class="title-area">
              <h4><%= rows[row].title; %></h4>
            </div>
            <p><%= rows[row].description; %></p>
            <a href="<%= rows[row].url; %>" target="_blank"><%= rows[row].title; %></a>
            <p><%= rows[row].numlikes; %><i class="far fa-heart"></i> </p>
          </div>
        </a>
      </div> 
        <% } %>
      </div>  

    </div>    

</body>





</html>
